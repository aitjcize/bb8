{
  "platforms": {
    "Facebook": "bb8.test.platform"
  },
  "bot": {
    "name": "postback",
    "description": "test postback",
    "interaction_timeout": 120,
    "admin_interaction_timeout": -1,
    "session_timeout": 864000,
    "settings": {},
    "nodes": {
      "Start": {
        "name": "Start",
        "description": "Start",
        "expect_input": false,
        "content_module": {
          "id": "ai.compose.content.core.text_message",
          "config": {
            "text": "In Start"
          }
        },
        "parser_module": {
          "id": "ai.compose.parser.core.passthrough",
          "config": {
            "end_node_id": "Root",
            "ack_message": "Goto Root"
          }
        }
      },
      "Root": {
        "name": "Root",
        "description": "Root",
        "expect_input": true,
        "content_module": {
          "id": "ai.compose.content.core.selection",
          "config": {
            "text": "question",
            "choices": [
              {
                "title": "option 1",
                "payload": "payload1",
                "acceptable_inputs": ["^A$", "^B$", "^C$"]
              },
              {
                "title": "option 2",
                "payload": "payload2",
                "acceptable_inputs": ["^D$", "^E$", "^F$"]
              }
            ]
          }
        },
        "parser_module": {
          "id": "ai.compose.parser.core.default",
          "config": {
            "links": [
              {
                "rule": {
                    "type": "regexp",
                    "params": ["payload1"]
                },
                "end_node_id": "Option1",
                "ack_message": "Goto Option1"
              },
              {
                "rule": {
                    "type": "regexp",
                    "params": ["payload2"]
                },
                "end_node_id": "Option2",
                "ack_message": "Goto Option2"
              }
            ]
          }
        }
      },
      "Option1": {
        "name": "Option1",
        "description": "Option1",
        "expect_input": true,
        "content_module": {
          "id": "ai.compose.content.test.show_response",
          "config": {
            "response": "{{text}}"
          }
        },
        "parser_module": {
          "id": "ai.compose.parser.core.passthrough",
          "config": {
            "end_node_id": "Root",
            "ack_message": "Goto Root"
          }
        }
      },
      "Option2": {
        "name": "Option2",
        "description": "Option2",
        "expect_input": true,
        "content_module": {
          "id": "ai.compose.content.test.show_response",
          "config": {
            "response": "{{text}}"
          }
        },
        "parser_module": {
          "id": "ai.compose.parser.core.passthrough",
          "config": {
            "end_node_id": "Root",
            "ack_message": "Goto Root"
          }
        }
      }
    }
  }
}
