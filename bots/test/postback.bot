{
  "platforms": {
    "Facebook": "bb8.test.platform"
  },
  "bot": {
    "name": "Postback",
    "description": "test Postback",
    "interaction_timeout": 120,
    "admin_interaction_timeout": -1,
    "session_timeout": 864000,
    "settings": {},
    "nodes": {
      "Start": {
        "name": "Start",
        "description": "Start",
        "expect_input": false,
        "content_module": {
          "id": "ai.compose.content.core.text_message",
          "config": {
            "text": "In Start"
          }
        },
        "parser_module": {
          "id": "ai.compose.parser.core.passthrough",
          "config": {
            "end_node_id": "Root",
            "ack_message": "Goto Root"
          }
        }
      },
      "Root": {
        "name": "Root",
        "description": "Root",
        "expect_input": true,
        "content_module": {
          "id": "ai.compose.content.core.text_message",
          "config": {
            "text": "In Root"
          }
        },
        "parser_module": {
          "id": "ai.compose.parser.core.default",
          "config": {
            "links": [
              {
                "rule": {
                    "type": "regexp",
                    "params": ["postback"]
                },
                "end_node_id": "Postback",
                "ack_message": "Goto Postback"
              }
            ]
          }
        }
      },
      "Postback": {
        "name": "Postback",
        "description": "Show and parse Postback",
        "expect_input": true,
        "content_module": {
          "id": "ai.compose.content.test.postback",
          "config": {
            "text": "In Postback"
          }
        },
        "parser_module": {
          "id": "ai.compose.parser.core.default",
          "config": {
            "links": [
              {
                "rule": {
                    "type": "regexp",
                    "params": [".*"]
                },
                "end_node_id": "Show",
                "ack_message": "Goto Show"
              },
              {
                "end_node_id": "Root",
                "ack_message": "Goto Root"
              }
            ]
          }
        }
      },
      "Show": {
        "name": "Show",
        "description": "Show response",
        "expect_input": true,
        "content_module": {
          "id": "ai.compose.content.test.show_response",
          "config": {
            "response": "{{text}}"
          }
        },
        "parser_module": {
          "id": "ai.compose.parser.core.passthrough",
          "config": {
            "end_node_id": "Root",
            "ack_message": "Goto Root"
          }
        }
      }
    }
  }
}
