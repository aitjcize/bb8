{
    "extends": "airbnb",
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "env": {
        "browser": true
    },
    "rules": {
      "no-constant-condition": 0,
      "semi": [2, "never"],
      "new-cap": 0,
      "jsx-a11y/no-static-element-interactions": 0
    }
}
